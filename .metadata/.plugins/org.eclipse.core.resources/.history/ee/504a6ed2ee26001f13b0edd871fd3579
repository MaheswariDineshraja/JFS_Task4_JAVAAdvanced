package com.advjava;

public class Voter extends Exception {
	
	private int voterID;
	private String voterName;
	private int voterAge;
	
	public Voter() {
		super();
	}
	
	public Voter(int voterid, String name, int voterage) {
		super();
		this.voterID = voterid;
		this.voterName = name;
		this.voterAge = voterage;
	}
	
	
